{
  "name": "web-patronat",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.preprod react-scripts start",
    "start-prod": "env-cmd -f .env.prod react-scripts start",
    "build": "react-scripts build",
    "build-preprod": "env-cmd -f .env.preprod react-scripts build",
    "build-prod": "env-cmd -f .env.prod react-scripts build",
    "deploy-preprod": "npm run build-preprod && firebase deploy --only hosting --project web-patronat-preprod",
    "deploy-prod": "npm run build-prod && firebase deploy --only hosting --project patronat-festes-roquetes",
    "cors-preprod": "gsutil cors set cors.json gs://web-patronat-preprod.firebasestorage.app",
    "cors-prod": "gsutil cors set cors.json gs://patronat-festes-roquetes.firebasestorage.app",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write .",
    "docs": "jsdoc -c jsdoc.config.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.12",
    "@eslint/js": "^9.18.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "14.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "storybook": "^10.1.4",
    "@storybook/react-webpack5": "^10.1.4",
    "@storybook/preset-create-react-app": "^10.1.4",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-onboarding": "^10.1.4",
    "webpack": "^5.103.0",
    "eslint-plugin-storybook": "^10.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/poppins": "^5.1.1",
    "@fontsource/roboto": "^5.1.1",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@mui/base": "^5.0.0-beta.68",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@mui/styled-engine-sc": "^6.4.0",
    "browser-image-compression": "^2.0.2",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.4",
    "env-cmd": "^10.1.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "firebase": "^11.1.0",
    "framer-motion": "^11.18.1",
    "i18next": "^23.16.8",
    "jsdoc": "^4.0.5",
    "jsencrypt": "^3.3.2",
    "loglevel": "^1.9.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^14.1.3",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.14",
    "styletron-engine-atomic": "^1.6.2",
    "styletron-react": "^6.1.1",
    "sweetalert2": "^11.15.10",
    "sweetalert2-react-content": "^5.1.0",
    "web-vitals": "^4.2.4"
  }
}
